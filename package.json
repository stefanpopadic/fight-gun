{
  "name": "stick-figure-arena",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc --project tsconfig.server.json",
    "preview": "vite preview",
    "start": "node dist/server/server/server.js",
    "dev:server": "tsx watch server/server.ts",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:server\""
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "express": "^4.18.2",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "tsx": "^4.6.2",
    "concurrently": "^8.2.2"
  }
}
